# platform :ios, '9.0'

target 'TodoWithFirebase' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  pod 'Firebase/Database'
  pod 'Firebase/RemoteConfig'


  # Pods for TodoWithFirebase

  target 'TodoWithFirebaseTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TodoWithFirebaseUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
